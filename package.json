{
  "name": "express-fantastic",
  "version": "1.0.0",
  "description": "Minimalist but fantastic MVC for express with es6 syntax",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "build": "babel src -d build",
    "serve": "node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "javascript",
    "express",
    "express-fantastic",
    "nodejs",
    "framework",
    "mvc",
    "api"
  ],
  "author": "El-Mahbub <elmahbub14021993@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "i18n": "^0.8.3",
    "jquery": "^3.3.1",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.4",
    "nodemon": "^1.17.3",
    "randombytes": "^2.0.6",
    "socket.io": "^2.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
